# ============================================================================
# ngrok 部署环境变量配置
# 使用方法: cp .env.example .env && 根据需要修改配置
# ============================================================================

# 环境标识
ENVIRONMENT=dev

# 服务域名配置
NGROK_DOMAIN=ngrok.local

# 端口映射配置
NGROK_HTTP_PORT=8080      # HTTP 公共端口
NGROK_HTTPS_PORT=8443     # HTTPS 公共端口
NGROK_TUNNEL_PORT=4443    # ngrok 客户端连接端口

# 日志配置
NGROK_LOG_LEVEL=INFO      # 日志级别: DEBUG, INFO, WARNING, ERROR

# Docker 构建配置
DOCKER_BUILDKIT=1         # 启用 BuildKit 加速构建
COMPOSE_PROJECT_NAME=ngrok

# 时区配置
TZ=Asia/Shanghai
